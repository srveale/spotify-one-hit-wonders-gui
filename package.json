{
  "name": "spotify-one-hit-wonders-gui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "d3": "^4.10.2",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-scripts": "1.0.13"
  },
  "apps": [
    {
      "name": "SPOT-OHW-GUI",
      "script": "",
      "args": "",
      "env": {
        "PORT": "3000"
      }
    }
  ],
  "deploy": {
    "production": {
      "user": "scott",
      "host": "scottveale.ca",
      "ref": "origin/master",
      "repo": "git@github.com:srveale/spotify-one-hit-wonders-gui",
      "path": "/home/scott/gui",
      "post-deploy": "npm install && npm run build && pm2 start node_modules/react-scripts/scripts/start.js --name SPOT-OHW-GUI",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "staging": {
      "user": "scott",
      "host": "scott.tillerdigital.ca",
      "ref": "origin/master",
      "repo": "git@github.com:srveale/spotify-one-hit-wonders-gui",
      "path": "/home/scott/gui",
      "post-deploy": "npm install && pm2 startOrRestart package.json --env dev",
      "env": {
        "NODE_ENV": "staging"
      }
    }
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "proxy": "http://localhost:3001"
}
